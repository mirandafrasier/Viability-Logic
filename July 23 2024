IF (
    (
        
        (CONTAINS(UPPER([Targeted_Provider]), UPPER('optimum'))
         AND CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
         AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
         AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
         AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('overlap'))
         AND CONTAINS(UPPER([Eligible_Provider]), UPPER('optimum'))
         AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Primary Eligible Provider]), UPPER('cox'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )

OR 
        
        (CONTAINS(UPPER([Targeted_Provider]), UPPER('cox'))
         AND CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
         AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
         AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
         AND CONTAINS(UPPER([Eligible_Provider]), UPPER('cox'))
         AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Primary Eligible Provider]), UPPER('optimum'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Targeted_Provider]), UPPER('frontier'))
            AND CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Eligible_Provider]), UPPER('frontier'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Primary Eligible Provider]), UPPER('frontier'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Targeted_Provider]), UPPER('spectrum'))
            AND CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Eligible_Provider]), UPPER('spectrum'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Primary Eligible Provider]), UPPER('spectrum'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Targeted_Provider]), UPPER('xfinity'))
            AND CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Eligible_Provider]), UPPER('xfinity'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Primary Eligible Provider]), UPPER('xfinity'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )

        OR 
        
        (
            CONTAINS(UPPER([Targeted_Provider]), UPPER('fidium'))
            AND CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Eligible_Provider]), UPPER('fidium'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
        OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('google'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('organic'))
            AND CONTAINS(UPPER([Primary Eligible Provider]), UPPER('fidium'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
 OR
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectacp'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('facebook'))
            AND CONTAINS(UPPER([Lead Reference]), UPPER('nationwide'))
            AND (CONTAINS(UPPER([Eligible_Provider]), UPPER('fidium')) OR CONTAINS(UPPER([Eligible_Provider]), UPPER('frontier')) OR CONTAINS(UPPER([Eligible_Provider]), UPPER('spectrum')) OR CONTAINS(UPPER([Eligible_Provider]), UPPER('optimum')))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))

 OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectus'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('facebook'))
            AND CONTAINS(UPPER([Targeted_Provider]), UPPER('optimum'))
            AND CONTAINS(UPPER([Campaign_Type]), UPPER('messenger'))
            AND CONTAINS(UPPER([Eligible_Provider]), UPPER('optimum'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
        
OR 
        
        (
            CONTAINS(UPPER([Source Reference]), UPPER('freeconnectacp'))
            AND CONTAINS(UPPER([Advertising_Platform]), UPPER('facebook'))
            AND NOT CONTAINS(UPPER([Lead Reference]), UPPER('nationwide'))
            AND CONTAINS(UPPER([Targeted_Provider]), UPPER('optimum'))
            AND CONTAINS(UPPER([Campaign_Type]), UPPER('leadgen'))
            AND CONTAINS(UPPER([Eligible_Provider]), UPPER('optimum'))
            AND (NOT CONTAINS(UPPER([Existing_Customer]), UPPER('True')) OR ISNULL([Existing_Customer]))
        )
